   ],
   "source": [
    "def convertTuple(tup):\n",
    "\t\t# initialize an empty string\n",
    "\tstr = ''\n",
    "\tfor item in tup:\n",
    "\t\tstr = str + item\n",
    "\treturn str\n",
    "\n",
    "\n",
    "# Driver code\n",
    "tuple = ('a', 'y', 'e', 's', 'h','a')\n",
    "str = convertTuple(tuple)\n",
    "print(str)\n"
   ]
